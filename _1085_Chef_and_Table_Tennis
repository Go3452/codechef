import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int j=0;j<t;j++)
	 {
	  String s=sc.next();
	  int c0=0,c1=0,n=s.length();
	  for(int i=0;i<n;i++)
	  {
	   if(s.charAt(i)=='1')
	    c1++;
	   else
	    c0++;
	  }
	  if(c1>c0)
	   System.out.println("WIN");
	  else
	   System.out.println("LOSE");
	 }
	}
}
