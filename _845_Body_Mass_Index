import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int m=sc.nextInt();
	  int n=sc.nextInt();
	  int Bmi=m/(n*n);
	  if(Bmi<=18)
	   System.out.println(1);
	  else if(Bmi<=24)
	   System.out.println(2);
	  else if(Bmi<=29)
	   System.out.println(3);
	  else
	   System.out.println(4);
	 }
	}
}
