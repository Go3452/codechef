import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int x = sc.nextInt();
	 int y = sc.nextInt();
	 int n = sc.nextInt();
	 n-=x;
	 n/=y;
	 if(n%2==0)
	  System.out.println("Lucky Chef");
	 else
	  System.out.println("Unlucky Chef");
	}
}
