import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int g = sc.nextInt();
	 int s = sc.nextInt();
	 int b = sc.nextInt();
	 int remain=0;
	 if(g<5) remain+=5-g;
	 if(s<5) remain+=5-s;
	 if(b<5) remain+=5-b;
	 System.out.println(remain);
	}
}
