import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int j=0;j<t;j++)
	 {
	  int n=sc.nextInt();
	  int arr[] = new int[n];
	  int max=0;
	  for(int i=0;i<n;i++)
	  {
	   arr[i]=sc.nextInt();
	   if(max<arr[i])
	    max=arr[i];
	  }
	  System.out.println(max);
	 }
	}
}
