import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int b=sc.nextInt();
	  int ls=sc.nextInt();
	  System.out.print(Math.sqrt(ls*ls-b*b) + " ");
	  System.out.println(Math.sqrt(b*b+ls*ls));
	 }
	}
}
