import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int j=0;j<t;j++)
	 {
	  int n=sc.nextInt();
	  int k=sc.nextInt();
	  int []arr = new int[n];
	  for(int i=0;i<n;i++)
	   arr[i]=sc.nextInt();
	  String s="";
	  for(int i=0;i<n;i++)
	  {
	   if(arr[i]%k==0)
	    s+="1";
	   else
	    s+="0";
	  }
	  System.out.println(s);
	 }
	}
}
