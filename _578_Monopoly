import java.util.Scanner;

class Codechef
{
	public static void main (String[] args)
	{
	  Scanner sc = new Scanner(System.in);
	  int t = sc.nextInt();
	  for(int i=0;i<t;i++)
	  {
	   int a,b,c,d;
	   a = sc.nextInt();
	   b = sc.nextInt();
	   c = sc.nextInt();
	   d = sc.nextInt();
	   if(a>b+c+d || b>a+c+d || c>a+b+d || d>a+b+c)
	    System.out.println("yes");
	   else
	    System.out.println("no");
	  }
	}
}
