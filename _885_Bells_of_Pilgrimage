import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int n=sc.nextInt();
	  int x=sc.nextInt();
	  int k=sc.nextInt();
	  int p=sc.nextInt();
	  if(k<=x)
	   System.out.println(p+10*k);
	  else if(k==n)
	   System.out.println(p+(k-x)*5+x*10+20);
	  else
	   System.out.println(p+(k-x)*5+x*10);
	 }
	}
}
