import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
     Scanner sc = new Scanner(System.in);
     int t = sc.nextInt();
     for(int i=0;i<t;i++)
     {
      int n=sc.nextInt();
      int a=sc.nextInt();
      int b=sc.nextInt();
      int m=(n+180)*2-(a+b);
      System.out.println(m);
     }
	}
}
