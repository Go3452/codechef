import java.util.Scanner;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int j=0;j<t;j++)
	 {
	  int a=sc.nextInt();
	  int b=sc.nextInt();
	  int c=sc.nextInt();
	 for(int i=2;i<200;i++)
	 {
	  if(a%i!=0)
	  {
	   if(b%i!=0)
	   {
	    if(c%i!=0)
	    {
	     System.out.println(i);
	     break;
	    }
	   }
	  }
	 }
	 }
	}
}
