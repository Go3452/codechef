import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
     Scanner sc = new Scanner(System.in);
     int t=sc.nextInt();
     for(int i=0;i<t;i++)
     {
      int k=sc.nextInt();
      int a=k%2,b=k/2;;
      System.out.println(b*2+a*3);
     }
	}
}
