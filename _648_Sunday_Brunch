import java.util.Scanner;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int x = sc.nextInt();
	  int y = sc.nextInt();
	  int c = x/y;
	  if(c<=20)
	   System.out.println(c);
	  else
	   System.out.println(20);
	 }
	}
}
