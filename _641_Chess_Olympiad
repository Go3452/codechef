import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int x = sc.nextInt();
	 int y = sc.nextInt();
	 int z = sc.nextInt();
	 int a=x*2+y;
	 int b=z*2+y;
	 a+=(4-x-y-z)*2;
	 if(a>b)
	  System.out.println("YES");
	 else
	  System.out.println("NO");
	}
}
