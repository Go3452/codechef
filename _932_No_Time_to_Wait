import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int n=sc.nextInt();
	 int h=sc.nextInt();
	 int x=sc.nextInt();
	 int []t = new int[n];
	 for(int i=0;i<n;i++)
	  t[i]=sc.nextInt();
	 String ans="NO";
	 for(int i=0;i<n;i++)
	 {
	  if(t[i]+x>=h)
	  {
	   ans="Yes";
	   break;
	  }
	 }
	 System.out.println(ans);
	}
}
