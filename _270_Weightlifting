import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int sum=0;
	 int a1=0,a2=0;
	 a1=sc.nextInt();
	 a2=sc.nextInt();
	 if(a1>a2) sum+=a1;
	 else sum+=a2;
	 a1=sc.nextInt();
	 a2=sc.nextInt();
	 if(a1>a2) sum+=a1;
	 else sum+=a2;
	 a1=sc.nextInt();
	 a2=sc.nextInt();
	 if(a1>a2) sum+=a1;
	 else sum+=a2;
	 System.out.println(sum);
	}
}
