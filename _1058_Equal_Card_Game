import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int k=sc.nextInt();
	  int count=0;
	  while(true)
	  {
	   if((52-count)%k==0)
	    break;
	   count++;
	  }
	  System.out.println(count);
	 }
	}
}
