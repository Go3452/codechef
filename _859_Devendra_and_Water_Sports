import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc=new Scanner(System.in);
	 int t=sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int z=sc.nextInt();
	  int y=sc.nextInt();
	  int a=sc.nextInt();
	  int b=sc.nextInt();
	  int c=sc.nextInt();
	  z-=y;
	  a+=b;
	  a+=c;
	  if(z<a)
	   System.out.println("NO");
	  else 
	   System.out.println("Yes");
	 }
	}
}
