import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{ 
	 Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int j=0;j<t;j++)
	 {
	  int n = sc.nextInt();
	  int []arr = new int[n];
	  int k = sc.nextInt();
	  int sum=0,count=0;
	  for(int i=0;i<n;i++)
	   arr[i] = sc.nextInt();
	  for(int i=0;i<n;i++)
	  {
	   sum+=arr[i];
	   if(sum<=k)
	    count++;
	   else
	    break;
	  }
	  System.out.println(count);
	 }
	}
}
