import java.util.Scanner;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
     Scanner sc= new Scanner(System.in);
     int t=sc.nextInt();
     for(int i=0;i<t;i++)
     {
      int n=sc.nextInt();
      n--;
      if(n%7==0)
       System.out.println(n/7);
      else
       System.out.println(n/7+1);
     }
	}
}
